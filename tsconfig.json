{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "removeComments": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "outDir": "./dist",
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "declaration": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}


